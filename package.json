{
  "author": "The Major",
  "name": "yew-app",
  "version": "0.1.0",
  "scripts": {
    "build": "rm dist/*; WASM_PACK_PROFILE=release parcel build static/manager.html --no-source-maps  --public-url ./",
    "start": "WASM_PACK_PROFILE=dev parcel static/manager.html -p 8000 --open",
    "test": "wasm-pack test --headless --chrome"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@types/file-saver": "^2.0.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-wasm.rs": "^1.2.16",
    "sass": "^1.26.11",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "file-saver": "^2.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": "> 0.5%, not IE 11"
          }
        }
      ]
    ]
  }
}
